{
  "name": "Cheems",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Cheems Cheems Cheems Cheems Cheems Cheems Cheems Cheems Cheems Cheems Cheems ",
  "homepage_url": "http://Cheems.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon20.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
